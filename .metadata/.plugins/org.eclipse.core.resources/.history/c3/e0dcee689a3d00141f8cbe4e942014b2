package pages;
import javax.swing.*;

import com.alee.laf.WebLookAndFeel;

import java.awt.*;
import java.awt.event.*;
public class pageHub extends JFrame{
	
		JTabbedPane contentPanel = new JTabbedPane();
		JPanel test1 = new JPanel();
		JPanel test2 = new JPanel();
		private final JButton btnNewButton = new JButton("New button");
		private final JRadioButton rdbtnNewRadioButton = new JRadioButton("New radio button");
		
		
		public pageHub() {
			getContentPane().add(contentPanel);
			contentPanel.add("Reserver",test1);
			
			test1.add(rdbtnNewRadioButton);
			contentPanel.add("Kart",test2);
			
			test2.add(btnNewButton);
		}

}

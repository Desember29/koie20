package pages;
import javax.imageio.ImageIO;
import javax.swing.*;

import java.awt.*;
import java.awt.image.BufferedImage;
import java.io.File;
import java.io.IOException;


public class iniPage extends JFrame{
	public iniPage() {
		
		JPanel contentPane = new JPanel();
		getContentPane().add(contentPane, BorderLayout.CENTER);
		contentPane.setLayout(null);
		
		JButton btnOk = new JButton("OK");
		btnOk.setBounds(150, 177, 89, 23);
		contentPane.add(btnOk);
		
		JTextField txtEmail = new JTextField();
		txtEmail.setBounds(10, 143, 364, 23);
		contentPane.add(txtEmail);
		
		JTextPane txtpnEpos = new JTextPane();
		txtpnEpos.setBackground(SystemColor.menu);
		txtpnEpos.setText("Epost adresse:");
		txtpnEpos.setBounds(10, 123, 176, 20);
		contentPane.add(txtpnEpos);
		
		JPanel imgPanel = new JPanel();
		imgPanel.setBounds(72, 11, 208, 101);
		contentPane.add(imgPanel);
		
		BufferedImage myPicture;
		try {
			myPicture = ImageIO.read(new File("/resources/koie_login.jpg"));
			//JLabel picLabel = new JLabel(new ImageIcon("C:\\Users\\admin\\git\\koie20\\koie.ui\\resources\\koie_login.jpg"));
			JLabel picLabel = new JLabel(new ImageIcon("\\resources\\koie_login.jpg"));
			imgPanel.add(picLabel);
			
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		

		
		
		
		
	}
}
